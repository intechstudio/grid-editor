{
  "name": "grid-editor",
  "productName": "Grid Editor",
  "description": "Powerful Editor software for Intech Studio products.",
  "version": "1.2.54",
  "engines": {
    "node": ">=18.16.1"
  },
  "main": "dist/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/intechstudio/grid-editor.git"
  },
  "devDependencies": {
    "@electron/notarize": "^2.1.0",
    "@playwright/test": "^1.42.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@types/node": "^20.11.21",
    "autoprefixer": "^10.4.12",
    "chai": "^4.3.4",
    "cross-env": "^7.0.2",
    "dotenv": "^14.2.0",
    "electron": "^26.2.0",
    "electron-builder": "^24.6.4",
    "electron-vite": "^1.0.27",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-load-config": "^4.0.1",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "rollup-plugin-copy": "^3.4.0",
    "svelte": "^4.2.0",
    "svelte-grid": "^1.11.2",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.1.8",
    "vite-plugin-commonjs": "^0.6.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@intechstudio/grid-uikit": "^0.0.10",
    "adm-zip": "^0.5.10",
    "chokidar": "^3.5.3",
    "discord.js": "^13.6.0",
    "electron-dl": "^3.4.1",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "eslint": "^7.23.0",
    "firebase": "^9.18.0",
    "fs-extra": "^10.0.0",
    "googleapis": "^100.0.0",
    "grid-protocol": "^1.2.52",
    "is-online": "^10.0.0",
    "mixpanel-browser": "^2.47.0",
    "monaco-editor": "^0.34.0",
    "node-disk-info": "^1.3.0",
    "node-fetch": "^2.6.7",
    "panzoom": "^9.3.0",
    "polka": "^0.5.2",
    "semver": "^7.5.4",
    "set-interval-async": "^3.0.3",
    "simple-discord-webhooks": "^2.1.0",
    "sirv": "^2.0.3",
    "svelte-easy-popover": "^1.0.0",
    "svelte-splitpanes": "^0.8.0",
    "svelte-watch-resize": "^1.0.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0",
    "vite": "^4.5.2",
    "ws": "^7.5.8"
  },
  "scripts": {
    "e:builder": "cross-env NOTARIZE=true electron-builder --config electron-builder-config.js --publish onTagOrDraft",
    "e:builder:nightly": "cross-env NOTARIZE=true DEBUG=electron-builder electron-builder --config electron-builder-config.js --publish never",
    "e:builder:local": "cross-env DEBUG=electron-builder electron-builder --config electron-builder-config.js --publish never",
    "export": "run-s gen:buildVars:prod s:build e:builder",
    "export:nightly": "run-s gen:buildVars:nightly s:build e:builder:nightly",
    "export:alpha": "run-s gen:buildVars:alpha s:build e:builder",
    "export:local": "run-s clean-up gen:buildVars:local s:build e:builder:local",
    "clean-up": "rm -rf dist && rm -rf dist-web && rm -rf build",
    "rebuild": "./node_modules/.bin/electron-rebuild.cmd",
    "s:build": "electron-vite build",
    "gen:buildVars:dev": "node ./build-scripts/dev-buildVars.js",
    "gen:buildVars:local": "node ./build-scripts/local-buildVars.js",
    "gen:buildVars:nightly": "node ./build-scripts/nightly-buildVars.js",
    "gen:buildVars:alpha": "node ./build-scripts/alpha-buildVars.js",
    "gen:buildVars:prod": "node ./build-scripts/prod-buildVars.js",
    "preinstall": "node ./build-scripts/check-node-version.js",
    "postinstall": "electron-builder install-app-deps",
    "test": "mocha",
    "electron:dev": "cross-env NODE_OPTIONS=--max_old_space_size=8198 electron-vite dev --watch",
    "electron-dev": "run-s gen:buildVars:dev electron:dev",
    "format": "prettier --write .",
    "web:dev": "vite dev --mode web",
    "web:build": "vite build --mode web",
    "web:deploy": "firebase deploy --only hosting",
    "web-dev": "run-s gen:buildVars:dev web:dev",
    "web-build": "run-s clean-up gen:buildVars:prod web:build",
    "web-deploy": "run-s web-build web:deploy"
  }
}
